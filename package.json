{
	"name": "@cigale/beamup",
	"version": "0.0.3",
	"description": "Typed client to send corrections to a CIGALE BeamUp server",
	"module": "dist/client.js",
	"types": "dist/client.d.ts",
	"type": "module",
	"license": "MIT",
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"prepare": "bun run init-dotenv.ts",
		"lint": "oxlint",
		"format": "prettier --write .",
		"check:clutter": "knip",
		"migrate": "bun run migrate.ts",
		"makemigration": "bunx drizzle-kit generate --name"
	},
	"devDependencies": {
		"@types/bun": "^1.2.22",
		"dotenv": "^17.2.2",
		"drizzle": "^1.4.0",
		"drizzle-arktype": "^0.1.3",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.5",
		"knip": "^5.63.1",
		"nanoid": "^5.1.5",
		"oxlint": "^1.16.0",
		"prettier": "^3.6.2"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"arktype": "^2.1.22"
	}
}
